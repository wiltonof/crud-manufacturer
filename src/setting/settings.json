{
  "projectName": "CRUD Manufactorer",
  "version": "0.0.1",
  "dev": {
    "database": {
      "dialect": "sqlite",
      "storage": "src/dao/database.sqlite",
      "forceSync": true
    },
    "server": {
      "port": 3000,
      "jwtSecret": "random-secret-password",
      "jwtExpiration": "1h",
      "host": "127.0.0.1",
      "routePrefix": "/api",
      "plugins": [
        "swagger"
      ],
      "uploadPath": "/public/img/",
      "hostSwagger": "127.0.0.1:3000",
      "scheme": "http"
    }
  },
